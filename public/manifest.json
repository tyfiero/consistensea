{
  "manifest_version": 3,
  "name": "ConsistenSea - New tab page for daily habits.",
  "short_name": "ConsistenSea",
  "description": "Beautiful nautical new tab page to help you be consistent with your habits.",
  "author": "Ty Fiero",
  "homepage_url": "https://github.com/tyfiero/consistensea/tree/master",
  "version": "1.0.1",
  "permissions": ["storage", "notifications", "alarms", "background"],
  "action": {
    "default_title": "ConsistenSea - New tab page for daily digital habits.",
    "default_popup": "index.html",
    "default_icon": {
      "32": "icon32.png"
    }
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; script-src-elem 'self' 'unsafe-inline';"
  }
}
